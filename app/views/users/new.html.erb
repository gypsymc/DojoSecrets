<form action="/users" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
	<input type="text" id="Name" name = "user[name]" placeholder="Name">
	<input type="text" id="Email" name = "user[email]" placeholder="Email">
	<input type="text" id="Password" name = "user[password]" placeholder="password">
	<input type="text" id="Password_Confirmation" name = "user[password_confirmation" placeholder="Password Confirmation">>
	<input type="submit" name="Join" value="Join">
</form>

<%= link_to "Login", new_session_path%>
<%= link_to "Secrets", secrets_path%>
